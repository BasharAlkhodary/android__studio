        <View
            android:layout_width="380dp"
            android:layout_height="1dp"
            android:id="@+id/v_v6"
            android:layout_below="@id/tv_setting5"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:background="#C1B1B7BD"
