            android:textColor="#B5090707"
            android:paddingRight="10dp"
            android:drawableEnd="@drawable/arrow_right_30"
            android:paddingTop="15dp"
            />

        <View
            android:layout_width="380dp"
            android:layout_height="1dp"
            android:id="@+id/v_v12"
            android:layout_below="@id/tv_setting11"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:background="#C1B1B7BD"
            />
    </RelativeLayout>
</ScrollView>