<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fillViewport="true"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"

        >
        <TextView
            android:id="@+id/tv_tt1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="10dp"
            android:drawableStart="@drawable/keyboard_backspace_40"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:id="@+id/v_v1"
            android:layout_marginTop="55dp"
            android:background="#CEE0F3"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:id="@+id/tv_Main"
            android:layout_below="@id/v_v1"
            android:text="الاعدادات"
            android:textSize="20sp"
            android:textColor="#9A1342"
            android:textStyle="bold"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:id="@+id/tv_setting1"
            android:layout_below="@id/tv_Main"
            android:text="اتصل بنا"
            android:textSize="18sp"
            android:textColor="#B5090707"
            android:paddingRight="10dp"
            android:drawableEnd="@drawable/arrow_right_30"
            android:paddingTop="15dp"
            />

        <View
            android:layout_width="380dp"
            android:layout_height="1dp"
            android:id="@+id/v_v2"
            android:layout_below="@id/tv_setting1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:background="#C1B1B7BD"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:id="@+id/tv_setting2"
            android:layout_below="@id/v_v2"
            android:text="عن بنك فلسطين"
            android:textSize="18sp"
            android:textColor="#B5090707"
            android:paddingRight="10dp"
            android:drawableEnd="@drawable/arrow_right_30"
            android:paddingTop="15dp"
            />

        <View
            android:layout_width="380dp"
            android:layout_height="1dp"
            android:id="@+id/v_v3"
            android:layout_below="@id/tv_setting2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:background="#C1B1B7BD"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:id="@+id/tv_setting3"
            android:layout_below="@id/v_v3"
            android:text="مواقع"
            android:textSize="18sp"
            android:textColor="#B5090707"
            android:paddingRight="10dp"
            android:drawableEnd="@drawable/arrow_right_30"
            android:paddingTop="15dp"
            />

        <View
            android:layout_width="380dp"
            android:layout_height="1dp"
            android:id="@+id/v_v4"
            android:layout_below="@id/tv_setting3"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:background="#C1B1B7BD"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
